<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="Hall" prefHeight="507.0" prefWidth="729.0" stylesheets="@../css/Hall.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.match.controller.ControllerHall">
   <VBox layoutX="-6.0" prefHeight="511.0" prefWidth="63.0">
      <Button fx:id="registerUser" mnemonicParsing="false" onAction="#registerUserEvent" prefHeight="25.0" prefWidth="20.0" style="-fx-background-image:url('../ico/user.png')">
         <graphic>
            <ImageView fitHeight="42.0" fitWidth="42.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../ico/user.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="newRoom" accessibleHelp="新建房间" mnemonicParsing="false" onAction="#newRoomBtnEvent" prefHeight="12.0" prefWidth="49.0" text="   +" />
      <Button fx:id="exitApp" mnemonicParsing="false" onAction="#exitEvent" prefHeight="12.0" prefWidth="42.0">
         <graphic>
            <ImageView fitHeight="42.0" fitWidth="42.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../ico/exit.png" />
               </image>
            </ImageView>
         </graphic></Button>
   </VBox>
   <ListView fx:id="roomList" layoutX="43.0" prefHeight="511.0" prefWidth="686.0" />
</AnchorPane>
